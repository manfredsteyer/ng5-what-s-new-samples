{
  "name": "flug-portal",
  "version": "1.0.0",
  "description": "Example Project",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:aot": "ng build --aot",
    "build:aot-german": "ng build --aot --i18n-file src/locale/messages.de.xlf --locale de --i18n-format xlf",
    "build:all": "npm run build && npm run build:prod && npm run build:aot && npm run build:aot-german",
    "ng": "ng",
    "start": "ng serve --port 8081",
    "start:no-animation": "ng serve --env no-animation",
    "test": "ng test",
    "test:single": "ng test --single-run true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:start": "ng e2e --port 4200 --env no-animation",
    "translate": "ng xi18n",
    "check": "npm run test:single && npm run e2e:start && npm run translate && npm run build:all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0-rc.3",
    "@angular/common": "^5.0.0-rc.3",
    "@angular/compiler": "^5.0.0-rc.3",
    "@angular/core": "^5.0.0-rc.3",
    "@angular/forms": "^5.0.0-rc.3",
    "@angular/http": "^5.0.0-rc.3",
    "@angular/platform-browser": "^5.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^5.0.0-rc.3",
    "@angular/router": "^5.0.0-rc.3",
    "@angular/upgrade": "^5.0.0-rc.3",
    "@ngx-translate/core": "8.0.0",
    "@ngx-translate/http-loader": "2.0.0",
    "angular-oauth2-oidc": "2.1.8",
    "bootstrap": "^3.3.7",
    "core-js": "2.5.1",
    "hammerjs": "2.0.8",
    "jquery": "3.2.1",
    "rxjs": "5.5.0",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0-rc.2",
    "@angular/compiler-cli": "^5.0.0-rc.3",
    "@types/jasmine": "2.6.0",
    "@types/node": "8.0.46",
    "codelyzer": "3.2.1",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "mkdirp": "0.5.1",
    "protractor": "5.2.0",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "typescript": "^2.5.3"
  }
}
